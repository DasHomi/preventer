{
  "schemaVersion": 1,
  "id": "preventer",
  "version": "${version}",
  "name": "Preventer",
  "description": "A mod that prevents you from doing dumb things by accident",
  "authors": ["DasHomi"],
  "contact": {
    "issues": "https://github.com/DasHomi/preventer/issues",
    "homepage" : "https://github.com/DasHomi/preventer"
  },
  "license": "MIT",
  "icon": "assets/preventer/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.dashomi.preventer.PreventerClient"
    ],
    "modmenu" : [
      "com.dashomi.preventer.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "preventer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.0",
    "minecraft": ">=1.21",
    "java": ">=17",
    "fabric": ">=0.100.0",
    "cloth-config": ">=15.0.0"
  },
  "suggests": {
    "modmenu": ">=11.0.0"
  },
  "accessWidener": "preventer.accesswidener",
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.modrinth": "https://modrinth.com/mod/preventer",
        "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/preventer"
      }
    }
  }
}
