{
  "schemaVersion": 1,
  "id": "preventer",
  "version": "${version}",
  "name": "Preventer",
  "description": "A mod that prevents you from doing dumb things by accident",
  "authors": ["DasHomi"],
  "contact": {
    "issues": "https://github.com/DasHomi/preventer/issues",
    "homepage" : "https://github.com/DasHomi/preventer"
  },
  "license": "MIT",
  "icon": "assets/preventer/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.dashomi.preventer.PreventerClient"
    ],
    "modmenu" : [
      "com.dashomi.preventer.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "preventer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.8",
    "minecraft": "1.18.*",
    "java": ">=17",
    "fabric": ">=0.44.0",
    "cloth-config": ">=6.2.57"
  },
  "suggests": {
    "modmenu": ">=3.0.0"
  },
  "accessWidener": "preventer.accesswidener"
}
